jQuery(document).ready((function(e){window.matchMedia("(max-width: 1024px)").matches&&(e(".burger").click((function(t){t.preventDefault(),e(this).toggleClass("is-active"),e(".nav-bottom, body").toggleClass("is-active"),e(".menu-item-has-children .nav-plus, .sub-menu").removeClass("is-active")})),e(".menu-item-has-children > .nav-plus").click((function(t){t.preventDefault(),e(this).toggleClass("is-active"),e(this).next(".sub-menu").toggleClass("is-active")})))}));